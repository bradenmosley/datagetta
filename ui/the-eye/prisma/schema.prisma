  Batter                  String?  @db.VarChar
  BatterTeam              String?  @db.VarChar
  hits                    BigInt?
  at_bats                 BigInt?
  strikes                 BigInt?
  walks                   BigInt?
  strikeouts              BigInt?
  homeruns                BigInt?
  extra_base_hits         BigInt?
  plate_appearances       BigInt?
  hit_by_pitch            BigInt?
  sacrifice               BigInt?
  total_bases             BigInt?
  on_base_percentage      Decimal? @db.Decimal
