  PitcherTeam               String? @db.VarChar
  total_strikeouts_pitcher  BigInt?
  total_walks_pitcher       BigInt?
  total_out_of_zone_pitches BigInt?
  misses_in_zone            BigInt?
  swings_in_zone            BigInt?
