  HorzRelAngle            Decimal?          @db.Decimal
  SpinRate                Decimal?          @db.Decimal
  SpinAxis                Decimal?          @db.Decimal
  Tilt                    String?           @db.VarChar
  RelHeight               Decimal?          @db.Decimal
  RelSide                 Decimal?          @db.Decimal
